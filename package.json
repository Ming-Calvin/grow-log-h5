{
  "name": "grow-log-h5",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "core-js": "^3.8.3",
    "js-cookie": "^3.0.5",
    "lib-flexible": "^0.3.2",
    "moment": "^2.30.1",
    "vant": "^2.13.2",
    "vconsole": "^3.15.1",
    "vue": "^2.6.14",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "px2rem-loader": "^0.1.9",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "vConsole"
        }
      ],
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never",
        { "beforeStatementContinuationChars": "always" }
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "vue/html-indent": [
        "error",
        2
      ],
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 1,
          "multiline": 1
        }
      ],
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
